<%- include('../partials/header.ejs') %>
<div>
  <div>
    <a href="/entries/new" class="link-dark text-decoration-none">Add an entry</a> |
    Sort by:
    <% qps.forEach((qp) => { %>
      <% if (qp !== 'none') { %>
        <a href="/entries?sort=<%= qp %>" class="link-dark text-decoration-none"><%= qp %></a>
        <%= qp !== 'newest' ? '|' : '' %>
      <% } %>
    <% }); %>
  </div>
  <div class="page">
    <% for (let i=0; i < entries.length; i++) { %>
      <div class="word">
        <a href="/entries/<%= entries[i]._id %>" class="link-dark text-decoration-none">
          <%= entries[i].headword %>
        </a>
      </div>
    <% } %>
  </div>
</div>
<%- include('../partials/footer.ejs') %>